{
  "daemon": true,
  "run": [
    {
      "method": "shell.run",
      "params": {
        "path": "app",
        "message": "uv run main.py",
        "on": [
          {
            "event": "/http:\\/\\/[0-9.:]+/",
            "return": true
          }
        ]
      }
    },
    {
      "method": "local.set",
      "params": {
        "url": "{{input.event[0]}}"
      }
    }
  ]
}